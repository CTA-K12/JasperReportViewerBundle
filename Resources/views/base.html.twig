<!DOCTYPE html>
    <html lang="{{ app.request.locale }}">
        <head>
            {% block head %}
                {% block meta %}
                    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" charset="utf-8" />
                    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
                {% endblock meta %}

                <title>Report Viewer</title>

                {% block stylesheets %}
                    {% stylesheets
                    '@MESDJasperReportViewerBundle/Resources/public/css/style.css'
                    filter='cssrewrite' %}
                        <link rel="stylesheet" href="{{ asset_url }}" rel="stylesheet" media="all" />
                    {% endstylesheets %}
                    {% stylesheets
                    '@MESDJasperReportViewerBundle/Resources/public/css/print.css'
                    filter='cssrewrite' %}
                        <link rel="stylesheet" href="{{ asset_url }}" rel="stylesheet" media="print" />
                    {% endstylesheets %}
                {% endblock stylesheets %}

            {% endblock head %}
        </head>

        <body>
            {% block body %}

                <header id="header">
                    {% block navbar %}
                        <nav id="nav" class="navbar navbar-default navbar-fixed-top" role="navigation">
                            {% block navbarContent %}
                            {% endblock navbarContent %}
                        </nav><!-- END nav, navbar -->
                    {% endblock navbar %}
                </header>

                <div id="container" class="wrapper">
                    {% block container %}
                    {% endblock container %}
                </div><!-- END container -->

                <!-- {% block javascripts %}
                    {% javascripts
                        '@MesdPresentationBundle/Resources/public/js/jquery/*'
                        '@MesdPresentationBundle/Resources/public/js/bootstrap/*'
                        '@MesdPresentationBundle/Resources/public/js/cookie/*'
                        '@MesdPresentationBundle/Resources/public/js/introjs/*'
                        '@MesdPresentationBundle/Resources/public/js/elementqueries/*'
                        '@MesdPresentationBundle/Resources/public/js/orchestra.js'
                        output='bundles/mesdpresentation/js/orchestra.min.js'
                        %}
                        <script type="text/javascript" src="{{ asset_url }}"></script>
                    {% endjavascripts %}
                {% endblock javascripts %} -->
            {% endblock body %}
        </body>

    </html>