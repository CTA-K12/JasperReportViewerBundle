<!DOCTYPE html>
    <html lang="{{ app.request.locale }}">
        <head>
            {% block head %}
                {% block meta %}
                    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" charset="utf-8" />
                    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
                {% endblock meta %}

                <title>Report Viewer</title>

                {% block stylesheets %}

                    <link rel="stylesheet" type="text/css" media="all" href="//cdnjs.cloudflare.com/ajax/libs/normalize/2.1.3/normalize.min.css">
                    <link rel="stylesheet" type="text/css" media="all" href="//cdnjs.cloudflare.com/ajax/libs/jqueryui/1.10.3/css/base/jquery.ui.all.min.css">
                    <link rel="stylesheet" type="text/css" media="all" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.3.2/css/bootstrap.min.css">

                    {% stylesheets
                    '@MESDJasperReportViewerBundle/Resources/public/css/style.css'
                    filter='cssrewrite' %}
                        <link rel="stylesheet" href="{{ asset_url }}" rel="stylesheet" media="all" />
                    {% endstylesheets %}
                    {% stylesheets
                    '@MESDJasperReportViewerBundle/Resources/public/css/print.css'
                    filter='cssrewrite' %}
                        <link rel="stylesheet" href="{{ asset_url }}" rel="stylesheet" media="print" />
                    {% endstylesheets %}
                {% endblock stylesheets %}

            {% endblock head %}
        </head>

        <body>
            {% block body %}
                <header id="header">
                    {% block navbar %}
                        <div id="nav" class="navbar navbar-fixed-top" role="navigation">
                            <div class="navbar-inner">
                                <a class="brand" href="#">Report Viewer</a>
                                <ul class="nav">
                                    {% block navlinks %}
                                        <li>
                                            <a title="Report Home" href="{{ url('MESDJasperReportViewerBundle_home') }}"><i class="icon-home"></i> Home</a>
                                        </li>
                                        <li>
                                            <a title="Report History" href="{{ url('MESDJasperReportViewerBundle_report_history') }}"><i class="icon-time"></i> History</a>
                                        </li>
                                    {% endblock navlinks %}
                                </ul>
                            </div>
                        </div><!-- END nav, navbar -->
                    {% endblock navbar %}
                </header>

                <div class="wrapper wrapper-outer">
                    {% block sidebar %}
                        <aside class="sidebar">
                            {% block sidebarContent %}
                            {% endblock sidebarContent %}
                        </aside>
                    {% endblock sidebar %}

                    <div class="wrapper-inner">
                        {% block container %}
                        {% endblock container %}
                    </div>
                </div><!-- END container -->

                {% block javascripts %}
                    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/modernizr/2.7.0/modernizr.min.js"></script>
                    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
                    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
                    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.3.2/js/bootstrap.min.js"></script>
                    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jstree/3.0.0/jstree.min.js"></script>
                    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/datatables/1.10.0/jquery.dataTables.min.js"></script>
                    {% javascripts
                        '@MESDJasperReportViewerBundle/Resources/public/js/*'
                        %}
                        <script type="text/javascript" src="{{ asset_url }}"></script>
                    {% endjavascripts %}
                {% endblock javascripts %}
            {% endblock body %}
        </body>

    </html>